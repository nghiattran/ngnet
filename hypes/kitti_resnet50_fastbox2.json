{
    "model": {
        "input_file": "../inputs/kitti_input.py",
        "architecture_file" : "../encoder/resnet.py",
        "objective_file" : "../decoder/new_fastBox.py",
        "optimizer_file" : "../optimizer/generic_optimizer.py",
        "evaluator_file" : "../evals/kitti_eval.py"
    },
    "data": {
        "train_file": "KittiBox/train.txt",
        "val_file": "KittiBox/val.txt",
        "truncate_data": false,
        "eval_cmd": "../submodules/KittiObjective2/./evaluate_object2",
        "label_dir": "KittiBox/training/label_2"
    },
    "logging": {
        "display_iter": 200,
        "eval_iter": 2000,
        "write_iter": 800,
        "save_iter": 2000,
        "image_iter": 150000
    },
    "arch": {
        "layers": 50,
        "early_feat": "block1"
    },
    "path": ["../incl"],
    "solver": {
        "opt": "Adam",
        "use_jitter": true,
        "rnd_seed": 1,
        "epsilon": 0.00001,
        "learning_rate": 1e-5,
        "learning_rate_step": null,
        "hungarian_iou": 0.35,
        "weights": "",
        "head_weights": [1.0, 0.1],
        "max_steps": 150000,
        "keep_prob": 0.4
    },
    "jitter_scale_min": 0.8,
    "jitter_scale_max": 1.2,
    "jitter_offset": 16,
    "noise": "speckle",
    "flip": true,
    "load_pretrained": true,
    "use_rezoom": true,
    "augment_level": 1,
    "use_mask": true,
    "biggest_box_px": 10000,
    "early_feat_channels": 2048,
    "rezoom_change_loss": "center",
    "rezoom_w_coords": [-0.25, 0, 0.25],
    "rezoom_h_coords": [-0.25, 0, 0.25],
    "image_height": 384,
    "image_width": 1248,
    "grid_width": 39,
    "grid_height": 12,
    "batch_size": 3,
    "tau": 0.35,
    "scale_down": 0.1,
    "focus_size": 1.8,
    "region_size": 30,
    "clip_norm": 1.0,
    "num_inner_channel": 500,
    "num_classes": 2,
    "detect_truck": false
}